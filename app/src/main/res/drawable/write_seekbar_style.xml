<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android= "http://schemas.android.com/apk/res/android">
    <item
        android:id="@android:id/background">
        <shape >
            <corners
                android:radius="4dp" />
            <stroke
                android:width="1dp"
                android:color="@color/_4cc8cdda" />
           <solid
               android:color="@color/_ffffff"/>
        </shape>
    </item>
    <item
        android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners
                    android:radius="4dp" />
                <stroke
                    android:width="1dp"
                    android:color="@color/_4cc8cdda" />
                <solid
                    android:color="@color/_ffffff"/>
            </shape>
        </clip>
    </item>
    <item
        android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                    android:radius="4dp" />
                <stroke
                    android:width="1dp"
                    android:color="@color/_4cc8cdda" />
                <solid
                    android:color="@color/_a6c6ff"/>
            </shape>
        </clip>
    </item>
</layer-list>
